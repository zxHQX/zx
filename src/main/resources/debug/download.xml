<?xml version="1.0" encoding="utf-8"?>
<powertime>
	<!-- 应用系统图片下载 -->
	<request id="1003.401" act="select" pathname="application/%id" file="%image" retType="download">
		<database id="systemDatabase" table="application">
			<select>
				<field name="id"/>
				<field name="image"/>
				<where>
					<field name="id" alias="id" type="int" default="0"/>
				</where>
			</select>
		</database>
	</request>
</powertime>
