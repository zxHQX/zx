        <entry key="cpsClassify.add.message">[{1}]</entry>
        <entry key="cpsClassify.add.type">[{2}]</entry>

        <!--删除字典信息-->
        <entry key="dict.del">删除字典信息</entry>
        <entry key="dict.del.message">[{1}]</entry>
        <entry key="dict.del.type">[{2}]</entry>
